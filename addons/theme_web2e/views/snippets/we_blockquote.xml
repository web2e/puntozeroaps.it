<?xml version="1.0" encoding="utf-8"?>
<odoo>

<template name="We Blockquote" id="we_blockquote">
    <blockquote class="we_blockquote o_animable blockquote">
        <img
        src="/theme_web2e/static/src/img/quote.png"
        class="we_blockquote_icon" alt="quote" />
